{
  "name": "myexpress",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "start-src-node": "node -r esm src/app.js",
    "start-build-node": "node build/app.js",
    "dev-src-esm": "nodemon -r esm src/app.js",
    "dev-src-babel": "nodemon --exec babel-node src/app.js",
    "local-babel": "env-cmd -f .env nodemon --exec babel-node src/app.js",
    "local-esm": "env-cmd -f .env nodemon -r esm src/app.js",
    "build": "echo 'Transpiling';rm -rf build/ && babel src --out-dir build --copy-files",
    "eslint": "eslint . --ext .js",
    "prettier-format": "prettier --loglevel error --write .",
    "prettier-check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "convert-excel-to-json": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "migrate-mongoose": "^4.0.0",
    "moment": "^2.29.4",
    "mongodb": "^4.12.1",
    "mongoose": "^6.7.3",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.8",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "jest": "^29.0.2",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "pre-commit": [
    "eslint",
    "prettier-check",
    "build"
  ]
}
